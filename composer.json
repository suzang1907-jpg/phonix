{
  "name": "blogger/blogger",
  "type": "project",
  "description": "Blogger",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "repositories": [
    {
        "type": "path",
        "url": "./dev/dev/laravel-assets"
    },
    {
      "type": "path",
      "url": "./dev/dev/laravel-highway"
  },
  {
    "type": "path",
    "url": "./dev/dev/laravel-vue"
},
{
  "type": "path",
  "url": "./dev/dev/php-actions"
},
{
  "type": "path",
  "url": "./dev/dev/php-sitemap"
}
],
  "require": {
    "php": "^8.3",
    "ext-exif": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "dev/laravel-assets": "@dev",
    "dev/laravel-highway": "@dev",
    "dev/laravel-vue": "@dev",
    "dev/php-actions": "@dev",
    "dev/php-sitemap": "@dev",
    "doctrine/dbal": "^3.1",
    "google/apiclient": "*",
    "google/apiclient-services": "*",
    "guzzlehttp/guzzle": "^7.8.1",
    "laravel/framework": "^10.4.1",
    "laravel/sanctum": "^3.2.1",
    "laravel/tinker": "^2.0",
    "matomo/device-detector": "^6.1",
    "predis/predis": "^2.2",
    "spatie/async": "^1.5",
    "spatie/image": "^3.6",
    "spatie/image-optimizer": "^1.7"
  },
  "require-dev": {
    "brianium/paratest": "^7.1.1",
    "fakerphp/faker": "^1.9.1",
    "friendsofphp/php-cs-fixer": "^3.0",
    "laravel/sail": "^1.30.1",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^7.2.0",
    "nunomaduro/phpinsights": "^2.0",
    "phpunit/phpunit": "^10.0.16"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
